{
  "$version": "1.1.0",
  "$id": "governance-record-icon.plugin",
  "$name": "governance-record-icon",
  "$vendor": "Alfresco Software, Ltd.",
  "$license": "LGPL-3.0",
  "$runtime": "1.8.0",
  "$description": "Record Icon Plugin",

  "features": {
    "documentList": {
      "files": [
        {
          "id": "app.files.thumbnail",
          "key": "$thumbnail",
          "type": "image",
          "sortable": false,
          "desktopOnly": false,
          "template": "app.records.columns.icon"
        }
      ]
    }
  }
}
